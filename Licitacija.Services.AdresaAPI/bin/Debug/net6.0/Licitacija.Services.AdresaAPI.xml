        <member name="M:Licitacija.Services.AdresaAPI.Controllers.AdresaController.CreateAdresa(Licitacija.Services.AdresaAPI.DTOs.Adresa.AdresaCreateDTO)">
            <summary>
            Kreira novu adresu.
            </summary>
            <param name="adresaDTO">Model adrese</param>
            <returns>Potvrda o kreiranoj adresi.</returns>
            <remarks>
            Primer zahteva za kreiranje nove adrese \
            POST /api/adresa \
            {     \
                "ulica": "test ulica", \
                "broj": "444", \
                "mesto": "test mesto", \
                "postanskiBroj": "21000", \
                "drzavaId": "4563cf92-b8d0-4574-9b40-a725f884da36" , \
            }
            </remarks>
            <response code="201">Vraća kreiranu adresu</response>
            <response code="500">Serverska greška</response>
        </member>
